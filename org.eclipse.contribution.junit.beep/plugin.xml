<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.eclipse.contribution.junit.beep"
   name="JUnit Error Beep Plug-in"
   version="1.0.0"
   provider-name="">

   <runtime>
      <library name="junitbeep.jar">
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.contribution.junit"/>
      <import plugin="org.eclipse.jdt.core"/>
   </requires>


   	<extension point="org.eclipse.contribution.junit.listeners">
       <listener
        class="org.eclipse.contribution.junit.beep.BeepingListener">
      </listener>
   </extension>

</plugin>
